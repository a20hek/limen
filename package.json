{
  "name": "limen-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "web",
    "extension"
  ],
  "scripts": {
    "dev": "bun run dev:web",
    "build": "bun run build:web",
    "lint": "bun run lint:web",
    "dev:web": "cd web && bun run dev",
    "build:web": "cd web && bun run build",
    "start:web": "cd web && bun run start",
    "lint:web": "cd web && bun run lint",
    "extension:pack": "cd extension && bun run pack"
  }
}
